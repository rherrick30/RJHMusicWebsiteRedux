{
  "name": "rjh_music_site_redux",
  "version": "1.0.0",
  "description": "A music management and streaming server using Cory House's awesome starter kit",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "clean:build": "rimraf ./build && mkdir build",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "start": "  run-p start:dev",
    "serve:build": "NODE_ENV=production node server.js"
  },
  "author": "Rob Herrick",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.21.2",
    "bootstrap": "4.3.1",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "http-proxy-middleware": "^0.19.1",
    "image-webpack-loader": "^8.1.0",
    "immer": "9.0.6",
    "open": "^7.1.0",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-modal": "^3.15.1",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-select": "^5.2.1",
    "react-sortable-hoc": "^1.11.0",
    "react-table": "^6.8.0",
    "react-toastify": "4.5.2",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "10.0.1",
    "css-loader": "6.7.3",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.15.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "^0.12.3",
    "jest": "24.5.0",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "2.7.2",
    "node-fetch": "^3.1.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "7.0.2",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "style-loader": "3.3.1",
    "url-loader": "^4.1.0",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "3.11.3"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rherrick30/RJHMusicWebsiteRedux"
  }
}
